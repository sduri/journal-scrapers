{
	"url": "http://ieeexplore\\.ieee\\.org/document/",
	"headless": true,
	"elements": {
		"abstract": {
		"selector": "//div[contains(concat(' ', @class, ' '), ' abstract-text ')]",
		"attribute": "text"
		},
	    "keyword": {
	      "selector": "//li[contains(concat(' ', @class, ' '), ' doc-all-keywords-list-item ')][1]//a",
	      "attribute": "text"
	    },
	    "doi": {
	      "selector": "//div[contains(concat(' ', @class, ' '), ' stats-document-abstract-doi ')]//a",
	      "attribute": "text"
	    }
	}
}